{
  "name": "@portals/api-client",
  "version": "0.1.0",
  "private": true,
  "description": "API client for accessing backend services in the portals monorepo.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rimraf dist .turbo node_modules",
    "docs": "typedoc --out docs src/index.ts"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^3.0.0",
    "xml-js": "^1.6.11",
    "zod": "^3.20.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/xml-js": "^1.0.0",
    "eslint": "^8.0.0",
    "@portals/eslint-config": "workspace:*",
    "jest": "^29.0.0",
    "msw": "^1.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false
}
